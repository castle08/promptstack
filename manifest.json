{
    "manifest_version": 3,
    "name": "PromptStack",
    "description": "Personal Prompt Library",
    "version": "1.0",
    "action": {
      "default_popup": "html/index.html",
      "default_icon": "images/icon128.png"
    },
    "permissions": [
        "storage"
      ],
      "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
  }